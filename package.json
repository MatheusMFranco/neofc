    "expo-status-bar": "~1.4.4",
    "lottie-react-native": "5.1.4",
    "mathjs": "^11.8.0",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-paper": "^5.6.0",
    "react-native-reanimated": "^3.0.2",
